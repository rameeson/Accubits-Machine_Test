<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css"
    integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">

  <title>Newsletter</title>
</head>
<header>
  <%- include("partials/header") %>
</header>
<body>
  <div class="container">
    <main role="main" class="container mt-4">
      <div class="jumbotron">
        <table>
          <tr>
            <td style="width:50%">
              <hr />
            </td>
            <td style="vertical-align:middle; text-align: center; color:red ">
              <h3><b>Newsletter</b></3>
            </td>
            <td style="width:50%">
              <hr />
            </td>
          </tr>
        </table>
        <div>
          <div class="row">

            <form action="/newsletter" method="POST" enctype="multipart/form-data">
              <div class="mb-3">
                <label for="lname" class="form-label"><strong>Upload CSV</strong></label>
                <input type="file" name="data" class="form-control" id="lname">
                <button type="submit" class="btn btn-primary ml-3 mt-4">Submit</button>

              </div>


            </form>

          </div>
        </div>

    </main>

  </div>

</body>
</nav>

